{
  "requiresCompatibilities": [
    "EC2"
  ],
  "containerDefinitions": [
    {
      "name": "redis",
      "image": "redis:3.2.7",
      "essential": true,
      "command": [
        "redis-server",
        "--requirepass",
        "redispass"
      ],
      "memory": 1000,
      "cpu": 400
    },
    {
      "name": "postgres",
      "image": "postgres:9.6",
      "environment": [
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5432"
        }
      ],
      "portMappings": [
        {
          "containerPort": 5432,
          "hostPort": 5432
        }
      ],
      "memory": 2000,
      "cpu": 700,
      "essential": true
    },
    {
      "name": "webserver",
      "image": "749785218022.dkr.ecr.eu-west-1.amazonaws.com/airflow:latest",
      "links": [
        "postgres",
        "redis"
      ],
      "environment": [
        {
          "name": "FERNET_KEY",
          "value": "46BKJoQYlPPOexq0OhDZnIlNepKFf87WFwLbfzqDDho="
        },
        {
          "name": "EXECUTOR",
          "value": "Celery"
        },
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "redispass"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5432"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "command": [
        "webserver"
      ],
      "cpu": 200,
      "memory": 500,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/airflow",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "webserver"
        }
      }
    },
    {
      "name": "scheduler",
      "image": "749785218022.dkr.ecr.eu-west-1.amazonaws.com/airflow:latest",
      "essential": true,
      "links": [
        "postgres",
        "redis"
      ],
      "environment": [
        {
          "name": "FERNET_KEY",
          "value": "46BKJoQYlPPOexq0OhDZnIlNepKFf87WFwLbfzqDDho="
        },
        {
          "name": "EXECUTOR",
          "value": "Celery"
        },
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "redispass"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5432"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        }
      ],
      "command": [
        "scheduler"
      ],
      "memory": 1000,
      "cpu": 300,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/airflow",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "scheduler"
        }
      }
    },
    {
      "name": "worker",
      "image": "749785218022.dkr.ecr.eu-west-1.amazonaws.com/airflow:latest",
      "essential": true,
      "links": [
        "postgres",
        "redis"
      ],
      "environment": [
        {
          "name": "FERNET_KEY",
          "value": "46BKJoQYlPPOexq0OhDZnIlNepKFf87WFwLbfzqDDho="
        },
        {
          "name": "EXECUTOR",
          "value": "Celery"
        },
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "redispass"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5432"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        }
      ],
      "command": [
        "worker"
      ],
      "memory": 1000,
      "cpu": 300,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/airflow",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "worker"
        }
      }
    }
  ],
  "family": "airflow",
  "volumes": [],
  "placementConstraints": [],
  "networkMode": "bridge",
  "taskRoleArn": "arn:aws:iam::749785218022:role/ecs-task-role"
}