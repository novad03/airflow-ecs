{
  "containerDefinitions": [
    {
      "name": "redis",
      "image": "redis:3.2.7",
      "essential": true,
      "command": [
        "redis-server", "--requirepass", "redispass"
      ],
      "memory": 1000,
      "cpu": 500
    },
    {
      "name": "postgres",
      "image": "postgres:9.6",
      "environment": [
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        }
      ],
      "portMappings": [
        {
          "containerPort": 5432,
          "hostPort": 5432
        }
      ],
      "memory": 2000,
      "cpu": 1000,
      "essential": true
    },
    {
      "name": "webserver",
      "image": "749785218022.dkr.ecr.eu-west-1.amazonaws.com/airflow",
      "links": [
        "postgres",
        "redis"
      ],
      "environment": [
        {
          "name": "LOAD_EX",
          "value": "n"
        },
        {
          "name": "FERNET_KEY",
          "value": "46BKJoQYlPPOexq0OhDZnIlNepKFf87WFwLbfzqDDho="
        },
        {
          "name": "EXECUTOR",
          "value": "Celery"
        },
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "redispass"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "command": [
        "webserver"
      ],
      "cpu": 200,
      "memory": 500,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "airflow",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "airflow-webserver"
        }
      }
    },
    {
      "name": "scheduler",
      "image": "749785218022.dkr.ecr.eu-west-1.amazonaws.com/airflow",
      "essential": true,
      "links": [
        "postgres",
        "redis"
      ],
      "environment": [
        {
          "name": "LOAD_EX",
          "value": "n"
        },
        {
          "name": "FERNET_KEY",
          "value": "46BKJoQYlPPOexq0OhDZnIlNepKFf87WFwLbfzqDDho="
        },
        {
          "name": "EXECUTOR",
          "value": "Celery"
        },
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "redispass"
        }
      ],
      "command": [
        "scheduler"
      ],
      "memory": 500,
      "cpu": 200
    },
    {
      "name": "worker",
      "image": "749785218022.dkr.ecr.eu-west-1.amazonaws.com/airflow",
      "essential": true,
      "links": [
        "postgres",
        "redis"
      ],
      "environment": [
        {
          "name": "FERNET_KEY",
          "value": "46BKJoQYlPPOexq0OhDZnIlNepKFf87WFwLbfzqDDho="
        },
        {
          "name": "EXECUTOR",
          "value": "Celery"
        },
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "redispass"
        }
      ],
      "command": [
        "worker"
      ],
      "memory": 500,
      "cpu": 200
    }
  ],
  "family": "airflow"
}