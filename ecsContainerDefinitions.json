{
  "containerDefinitions": [
    {
      "name": "redis",
      "image": "redis:3.2.7",
      "essential": true,
      "command": [
        "redis-server",
        "--requirepass",
        "redispass"
      ],
      "memory": 1000,
      "cpu": 500
    },
    {
      "name": "postgres",
      "image": "postgres:9.6",
      "environment": [
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5432"
        }
      ],
      "portMappings": [
        {
          "containerPort": 5432,
          "hostPort": 5432
        }
      ],
      "memory": 2000,
      "cpu": 1000,
      "essential": true
    },
    {
      "name": "webserver",
      "image": "749785218022.dkr.ecr.eu-west-1.amazonaws.com/airflow",
      "links": [
        "postgres",
        "redis"
      ],
      "environment": [
        {
          "name": "LOAD_EX",
          "value": "n"
        },
        {
          "name": "FERNET_KEY",
          "value": "46BKJoQYlPPOexq0OhDZnIlNepKFf87WFwLbfzqDDho="
        },
        {
          "name": "EXECUTOR",
          "value": "Celery"
        },
        {
          "name": "POSTGRES_USER",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "airflow"
        },
        {
          "name": "POSTGRES_DB",
          "value": "airflow"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "redispass"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5432"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        }
      ],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "command": [
        "webserver"
      ],
      "cpu": 200,
      "memory": 500,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "airflow-webserver",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "webserver"
        }
      }
    },
